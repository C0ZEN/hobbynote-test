html(
ng-app="hobbynoteTestApp"
ng-strict-di="true"
lang="fr")
	head
		base
		meta(
		charset="utf-8")
		meta(
		http-equiv="X-UA-Compatible"
		content="IE=9; IE=8; IE=7; IE=EDGE")
		meta(
		name="viewport"
		content="width=device-width, initial-scale=1")
		link(
		rel="manifest"
		href="manifest.json")
		meta(
		name="theme-color"
		content="#FFFFFF")
		link(
		rel="canonical"
		href="<!-- @echo configJson.app.canonicalUrl -->")
		meta(
		property="og:url"
		content="<!-- @echo configJson.app.canonicalUrl -->")
		meta(property="og:title"
		content="Hobbynote Test App")
		meta(
		name="author"
		content="Geoffrey TESTELIN")
		meta(
		property="og:locale"
		content="fr_FR")
		meta(
		name="description"
		content="Just a test for Hobbynote")
		meta(
		property="og:description"
		content="Just a test for Hobbynote")
		meta(
		property="og:site_name"
		content="Hobbynote Test App")
		
		meta(
		name="twitter:card"
		content="summary")
		meta(
		name="twitter:site"
		content="@C0ZEN")
		meta(
		name="twitter:creator"
		content="@C0ZEN")
		
		script(
		type="application/ld+json"
		src="structured-data.json")

		script(type="text/javascript").
			newDeferredStylesheet = function ($stylesheetUrl) {
				var stylesheet  = document.createElement('link');
				stylesheet.rel  = 'stylesheet';
				stylesheet.href = $stylesheetUrl;
				stylesheet.type = 'text/css';
				document.getElementsByTagName('head')[0].appendChild(stylesheet);
			}

		<!-- @if target="dev" -->
		link(
		rel="stylesheet"
		href="styles/css/init-loader.min.css")
		script(type="text/javascript").
			newDeferredStylesheet('styles/css/vendors.min.css');
			newDeferredStylesheet('styles/css/hobbynote-test-app.min.css');
		<!-- @endif -->

		<!-- @if target="test" -->
		link(
		rel="stylesheet"
		href="styles/css/init-loader.min.css")
		script(type="text/javascript").
			newDeferredStylesheet('styles/vendors.min.css');
			newDeferredStylesheet('styles/hobbynote-test-app.min.css');
		<!-- @endif -->

		<!-- @if target="preprod" -->
		link(
		rel="stylesheet"
		href="styles/css/init-loader.min.css")
		script(type="text/javascript").
			newDeferredStylesheet('styles/vendors.min.css');
			newDeferredStylesheet('styles/hobbynote-test-app.min.css');
		<!-- @endif -->

		<!-- @if target="prod" -->
		link(
		rel="stylesheet"
		href="styles/css/init-loader.min.css")
		script(type="text/javascript").
			newDeferredStylesheet('styles/vendors.min.css');
			newDeferredStylesheet('styles/hobbynote-test-app.min.css');
		<!-- @endif -->

		<!-- Favicon -->
		link(
		rel="shortcut icon"
		href="images/ico/favicon.ico"
		type="image/x-icon")
		link(
		rel="apple-touch-icon"
		href="images/png/icon/apple-touch-icon.png")
		link(
		rel="apple-touch-icon"
		sizes="57x57"
		href="images/png/icon/apple-touch-icon-57x57.png")
		link(
		rel="apple-touch-icon"
		sizes="72x72"
		href="images/png/icon/apple-touch-icon-72x72.png")
		link(
		rel="apple-touch-icon"
		sizes="76x76"
		href="images/png/icon/apple-touch-icon-76x76.png")
		link(
		rel="apple-touch-icon"
		sizes="114x114"
		href="images/png/icon/apple-touch-icon-114x114.png")
		link(
		rel="apple-touch-icon"
		sizes="120x120"
		href="images/png/icon/apple-touch-icon-120x120.png")
		link(
		rel="apple-touch-icon"
		sizes="144x144"
		href="images/png/icon/apple-touch-icon-144x144.png")
		link(
		rel="apple-touch-icon"
		sizes="152x152"
		href="images/png/icon/apple-touch-icon-152x152.png")
		link(
		rel="apple-touch-icon"
		sizes="180x180"
		href="images/png/icon/apple-touch-icon-180x180.png")

	body(
	class="no-overflow")

	.altran-container.margin-l-top
		h1 Hi Captain !
		p Everything seems fine to me.
		pre
			span If you can read this message, your project is alive !
			br
			span It means that your are not dead. Yet.

	<!-- Main view -->
	ui-view

	<!-- Vendors JS -->
	script(
	type="text/javascript"
	defer
	src="scripts/vendors.min.js")

	<!-- @if target="dev" -->
	script(
	type="text/javascript"
	defer
	src="scripts/hobbynote-test-app.min.js")
	<!-- @endif -->

	<!-- @if target="test" -->
	script(
	type="text/javascript"
	defer
	src="scripts/hobbynote-test-app.min.js")
	<!-- @endif -->

	<!-- @if target="preprod" -->
	script(
	type="text/javascript"
	defer
	src="scripts/hobbynote-test-app.min.js")
	<!-- @endif -->

	<!-- @if target="prod" -->
	script(
	type="text/javascript"
	defer
	src="scripts/hobbynote-test-app.min.js")
	<!-- @endif -->

	<!-- No script message -->
	noscript
		span This app require Javascript to be enabled.
		span Make sure to turn it on.