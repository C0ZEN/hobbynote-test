//- Main text
span.screen-text(
dir-terminal-type
start-typing="{{ vm.data.isReady }}") {{ vm.computerScreenText }}

//- Wait input
.wait-input(
ng-if="vm.computerScreenWaitInput")

	//- Input
	input(
	ng-model="vm.waitInputModel"
	autofocus="true"
	autocomplete="off"
	spellcheck="false"
	tabindex="0")